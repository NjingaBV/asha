{
	"name": "@njingabv/asha",
	"version": "0.0.42",
	"svelte": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"type": "module",
	"publishConfig": {
		"registry": "https://npm.pkg.github.com"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/NjingaBV/asha.git"
	},
	"bugs": {
		"url": "https://github.com/NjingaBV/asha/issues"
	},
	"packageManager": "pnpm@8.6.3",
	"engines": {
		"pnpm": "^8.0.0"
	},
	"scripts": {
		"dev": "vite dev",
		"build": "svelte-kit sync && pnpm package",
		"preview": "vite preview",
		"package": "svelte-kit sync && svelte-package && NODE_ENV=production postcss src/app.css -o dist/asha.css && publint",
		"prepublishOnly": "pnpm run package",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test:unit": "vitest",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"storybook": "storybook dev -p 6006",
		"build-storybook": "storybook build",
		"release": "changeset publish",
		"publish": "npm publish",
		"postinstall": "xstate typegen \"./src/**/*.ts?(x)\""
	},
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js"
		}
	},
	"files": [
		"dist",
		"!dist/**/*.test.*",
		"!dist/**/*.spec.*"
	],
	"peerDependencies": {
		"svelte": "^3.55.0"
	},
	"devDependencies": {
		"@babel/core": "^7.22.5",
		"@storybook/addon-actions": "^7.0.22",
		"@storybook/addon-essentials": "^7.0.22",
		"@storybook/addon-links": "^7.0.22",
		"@storybook/addon-mdx-gfm": "^7.0.22",
		"@storybook/addon-postcss": "^2.0.0",
		"@storybook/addon-svelte-csf": "^3.0.3",
		"@storybook/addon-viewport": "^7.0.22",
		"@storybook/svelte": "^7.0.22",
		"@storybook/sveltekit": "^7.0.22",
		"@sveltejs/adapter-auto": "^2.1.0",
		"@sveltejs/kit": "^1.20.4",
		"@sveltejs/package": "^2.0.2",
		"@sveltejs/vite-plugin-svelte": "^2.4.1",
		"@tailwindcss/line-clamp": "^0.4.4",
		"@tsconfig/svelte": "^4.0.1",
		"@types/youtube": "^0.0.47",
		"@typescript-eslint/eslint-plugin": "^5.60.0",
		"@typescript-eslint/parser": "^5.60.0",
		"@xstate/cli": "^0.5.0",
		"@xstate/svelte": "^2.0.1",
		"@xstate/test": "^0.5.1",
		"autoprefixer": "^10.4.14",
		"babel-loader": "^9.1.2",
		"cssnano": "^6.0.1",
		"eslint": "^8.43.0",
		"eslint-config-prettier": "^8.8.0",
		"eslint-plugin-storybook": "^0.6.12",
		"eslint-plugin-svelte3": "^4.0.0",
		"postcss": "^8.4.24",
		"postcss-cli": "^10.1.0",
		"prettier": "^2.8.8",
		"prettier-plugin-svelte": "^2.10.1",
		"publint": "^0.1.12",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"storybook": "^7.0.22",
		"svelte": "^3.59.1",
		"svelte-check": "^3.4.3",
		"svelte-loader": "^3.1.8",
		"svelte-preprocess": "^5.0.4",
		"svelte2tsx": "^0.6.15",
		"tailwindcss": "^3.3.2",
		"tslib": "^2.5.3",
		"typescript": "^5.1.3",
		"vite": "^4.3.9",
		"vitest": "^0.32.2",
		"xstate": "^4.37.2"
	}
}
