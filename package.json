{
	"name": "@njingabv/asha",
	"version": "0.0.30",
	"main": "index.js",
	"svelte": "index.js",
	"types": "lib/index.d.ts",
	"type": "module",
	"publishConfig": {
		"registry": "https://npm.pkg.github.com"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/NjingaBV/asha.git"
	},
	"bugs": {
		"url": "https://github.com/NjingaBV/asha/issues"
	},
	"packageManager": "pnpm@7.26.3",
	"engines": {
		"pnpm": "^7.25.0"
	},
	"scripts": {
		"dev": "vite dev",
		"build": "svelte-kit sync && svelte-package && NODE_ENV=production postcss src/app.css -o package/asha.css",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test:unit": "vitest",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"storybook": "start-storybook -p 6006",
		"build-storybook": "build-storybook",
		"release": "changeset publish",
		"publish": "npm publish"
	},
	"exports": {
		"./asha.css": "./asha.css"
	},
	"devDependencies": {
		"@babel/core": "^7.20.7",
		"@storybook/addon-actions": "^6.5.15",
		"@storybook/addon-essentials": "^6.5.15",
		"@storybook/addon-links": "^6.5.15",
		"@storybook/addon-postcss": "^2.0.0",
		"@storybook/addon-svelte-csf": "^2.0.10",
		"@storybook/addon-viewport": "^6.5.15",
		"@storybook/svelte": "^6.5.15",
		"@sveltejs/adapter-auto": "^1.0.0",
		"@sveltejs/kit": "^1.0.1",
		"@sveltejs/package": "^1.0.1",
		"@sveltejs/vite-plugin-svelte": "^2.0.2",
		"@tailwindcss/line-clamp": "^0.4.2",
		"@tsconfig/svelte": "^3.0.0",
		"@typescript-eslint/eslint-plugin": "^5.47.1",
		"@typescript-eslint/parser": "^5.47.1",
		"autoprefixer": "^10.4.13",
		"babel-loader": "^9.1.0",
		"cssnano": "^5.1.14",
		"eslint": "^8.30.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"postcss": "^8.4.20",
		"postcss-cli": "^10.1.0",
		"prettier": "^2.8.1",
		"prettier-plugin-svelte": "^2.9.0",
		"svelte": "^3.55.0",
		"svelte-check": "^3.0.1",
		"svelte-loader": "^3.1.4",
		"svelte-preprocess": "^5.0.0",
		"svelte2tsx": "^0.6.0",
		"tailwindcss": "^3.2.4",
		"tslib": "^2.4.1",
		"typescript": "^4.9.4",
		"vite": "^4.0.3",
		"vitest": "^0.28.0"
	},
	"dependencies": {
		"@types/youtube": "^0.0.47"
	}
}
